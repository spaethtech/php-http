{
    "name": "spaethtech/php-http",
    "description": "Some common HTTP libraries.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@spaethtech.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",

        "ext-json": "*",

        "spaethtech/php-support": "*",

        "slim/slim": "~4",
        "twig/twig": "~3",
        "slim/twig-view": "~3"
    },
    "require-dev": {
        "phpunit/phpunit": "~9",
        "vlucas/phpdotenv": "~2.5",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "SpaethTech\\HTTP\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpaethTech\\HTTP\\": "./tests/"
        }
    }

}
